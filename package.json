{
  "name": "flappybird",
  "version": "1.0.0",
  "description": "This project is a clone of the popular mobile game \"flappy bird\" to be played in the browser.",
  "main": "index.js",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "production": "webpack --mode production --config webpack.config.js",
